TLDEPS=bindir mods libs libiotk pw ph

all : tldeps nmr.x

nmr.x : $(NMROBJ) $(PHOBJS) $(PWOBJS) $(MODULES) $(LIBOBJS)
	$(F90) $(LDFLAGS) -o nmr.x \
		$(NMROBJ) $(PHOBJS) $(PWOBJS) $(MODULES) $(LIBOBJS) $(LIBS)
tldeps:
	test -n "$(TLDEPS)" && ( cd .. ; $(MAKE) $(MFLAGS) $(TLDEPS) || exit 1) || :

clean :
	- /bin/rm -f nmr.x *.o *~ *.F90 *.d *.mod *.i work.pc

include make.depend
