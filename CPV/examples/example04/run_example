$ECHO "  running the calculation with 128 H2O molecules...\c"
if test "0$1" -ge 128 ; then
    $FPMD_COMMAND < h2o-128.in > h2o-128.out
    check_failure $?
    $ECHO " done"
else
    $ECHO " skipped" # too big, don't run
fi

