 /
 &system
    ibrav = 14,
    celldm(1) = 18.65,
    celldm(2) = 1.0,
    celldm(3) = 2.0,
    celldm(4) = 0.0,
    celldm(5) = 0.0,
    celldm(6) = 0.0,
    nat = 192,
    ntyp = 2,
    nbnd = 256,
    ecutwfc = 70.0,
    ecfixed = 68.0,
    qcutz = 68.0,
    q2sigma = 8.0,
    input_dft = 'BLYP',
 /
 &electrons
    emass = 400.d0,
    emass_cutoff = 2.5d0,
