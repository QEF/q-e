#define F77_FUNC(name,NAME) NAME ## _
_ACEOF

          cat >>confdefs.h <<\_ACEOF
#define F77_FUNC_(name,NAME) NAME ## _
_ACEOF
 ;;
  "upper case, underscore, extra underscore")
          cat >>confdefs.h <<\_ACEOF
